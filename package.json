{
  "name": "jsonforms-ui-schema-editor",
  "version": "0.1.0",
  "repository": "https://github.com/eclipsesource/jsonforms-ui-schema-editor",
  "bugs": "https://github.com/eclipsesource/jsonforms-ui-schema-editor/issues",
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts-ts": "2.14.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --project tsconfig.json --exclude src/registerServiceWorker.ts",
    "test": "jest --coverage",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^7.0.60",
    "@types/react": "^16.3.6",
    "@types/react-dom": "^16.0.4",
    "coveralls": "^3.0.0",
    "jest": "^22.4.3",
    "nyc": "^11.6.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
